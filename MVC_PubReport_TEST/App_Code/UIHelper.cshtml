    @functions {

        public static IHtmlString isHasChild(int MenuID, List<TFunctionList> list)
        {
            HtmlString html = new HtmlString("");
             
            foreach (var item in list)
            {
                if (MenuID == item.ParentMenuId)
                {
                    html = new HtmlString("Y");
                    break;
                }
                 
                    
            }
            return html;
        }
        
        
        
        
        
        
